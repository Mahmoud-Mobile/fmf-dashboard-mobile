{
  "expo": {
    "name": "Lead Ops",
    "slug": "fmf-guest-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "mode": "production",
          "iosDisplayInForeground": true
        }
      ],
      ["expo-screen-orientation", { "initialOrientation": "PORTRAIT" }]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.lead.fmfmanagement",
      "buildNumber": "29",
      "googleServicesFile": "./GoogleService-Info.plist",

      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show your current position and provide directions to event locations.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to show your current position and provide directions to event locations.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesIcloudStorage": false
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/icon.png",
      "orientation": "portrait",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lead.fmfmanagement",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["RECEIVE_BOOT_COMPLETED", "VIBRATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fcd7859c-877f-4e8f-b6ec-7059a611585e"
      }
    },
    "owner": "valueuro"
  }
}
